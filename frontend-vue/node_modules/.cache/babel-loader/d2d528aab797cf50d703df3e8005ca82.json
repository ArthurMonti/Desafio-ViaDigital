hen((res) => {\r\n              this.User.id = res.data.id;\r\n              this.User.name = res.data.name;\r\n              this.User.password = \"\";\r\n              this.$emit(\"logado\",{User: this.User});\r\n              this.$router.push({name: 'Board', params:{User: this.User}});\r\n          })\r\n          .catch((error) => {\r\n            console.log(error)\r\n            alert(error);\r\n          });   \r\n        }\r\n          \r\n          \r\n        },\r\n        Teste: function()\r\n        {\r\n          this.$emit(\"teste\",{title:\"test\"});\r\n        }\r\n    },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.btn{ margin-top: 4%}\r\n</style>"],"sourceRoot":"src/components"}]}