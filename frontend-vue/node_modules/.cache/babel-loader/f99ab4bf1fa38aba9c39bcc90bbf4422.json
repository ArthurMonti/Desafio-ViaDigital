n is-half is-offset-one-quarter\">\r\n    <div class=\"box\">\r\n      <form>\r\n        <div class=\"field\">\r\n          <label class=\"label is-left\">Nome Completo</label>\r\n          <div class=\"control\">\r\n            <input :class=\"{'is-danger input': isEmpty && User.name == '' ,'input' : true}\" type=\"text\" placeholder=\"Nome\" v-model=\"User.name\" />\r\n            <p v-if=\" isEmpty && User.name == '' \" style=\"text-align: left\" class=\"help is-danger\">Preencha este campo</p>\r\n          </div>\r\n        </div>\r\n        <br />\r\n        <div class=\"field\">\r\n          <label class=\"label\">Nome de Usuário</label>\r\n          <div class=\"control\">\r\n            <input :class=\"{'is-danger input': isEmpty && User.username == '' ,'input' : true}\" type=\"text\" placeholder=\"Usuário\" v-model=\"User.username\" />\r\n            <p v-if=\" isEmpty && User.username == '' \" style=\"text-align: left\" class=\"help is-danger\">Preencha este campo</p>\r\n          </div>\r\n        </div>\r\n        <br />\r\n        <div class=\"field\">\r\n          <label class=\"label\">Senha</label>\r\n          <div class=\"control\">\r\n            <input  :class=\"{'is-danger input': isEmpty && User.password == '' ,'input' : true}\" type=\"password\" placeholder=\"********\" v-model=\"User.password\" />\r\n            <p v-if=\" isEmpty && User.password == '' \" style=\"text-align: left\" class=\"help is-danger\">Preencha este campo</p>\r\n          </div>\r\n        </div>\r\n      </form>\r\n      <br>\r\n      <button class=\"button is-primary btn\" @click=\"Register\">Registrar</button>\r\n    </div>\r\n    <div class=\"notification is-success footer\">\r\n    <button class=\"delete\"></button>\r\n      Registro realizado com <strong>Sucesso!</strong>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nexport default {\r\n  components: {},\r\n  data() {\r\n    return {\r\n      User:{\r\n        name: \"\",\r\n        username: \"\",\r\n        password: \"\",\r\n      },\r\n      isEmpty: false\r\n      \r\n    };\r\n  },\r\n  methods: {\r\n    Register: function () {\r\n      if(this.User.name != \"\" || this.User.username != \"\" || this.User.password != \"\")\r\n      {\r\n        this.isEmpty = false;\r\n        axios.post(\"https://localhost:44360/Usuarios\",this.User)\r\n        .then((res) => {\r\n            console.log(\"registrou\", res.statusText);\r\n        })\r\n        .catch(() => {\r\n          console.log(\"Deu errado\");\r\n        });   \r\n      }\r\n      else\r\n      {\r\n        this.isEmpty = true;\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.footer{\r\nposition: absolute;\r\nbottom: 0;\r\n}\r\n\r\n</style>"],"sourceRoot":"src/components"}]}